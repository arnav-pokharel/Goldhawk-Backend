model startup_active {
  uid                           String  @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  company_name                  String?
  hq_address                    String?
  deal_amount                   String?
  pitch1_description            String?
  pitch2_description            String?
  pitch3_description            String?
  stack_1                       String?
  stack_2                       String?
  stack_3                       String?
  stack_4                       String?
  pitch1_link                   String?
  pitch2_link                   String?
  pitch3_link                   String?
  pitch_deck                    String?
  metric_1                      Json?   @db.Json
  metric_2                      Json?   @db.Json
  metric_3                      Json?   @db.Json
  logo                          String?
  metric_4                      String?
  metric_5                      String?
  metric_6                      String?
  metric_7                      String?
  website_url                   String?
  comp_dis                      String?
  thumb_url                     String?
  metric_8                      String?
  views                         String?
  termsheet_notes               Json?   @db.Json
  termsheet_safe                Json?   @db.Json
  termsheet_equity              Json?   @db.Json
  admin_founder_name            String?
  admin_founder_profile_picture String?
}

model vc_administrative {
  id          String      @id
  uid         String
  admin_uid   String
  createdAt   DateTime    @default(now())
  updatedAt   DateTime    @default(now())
  vc_partners vc_partners @relation(fields: [admin_uid], references: [id], onDelete: Cascade)
  vc_firms    vc_firms    @relation(fields: [uid], references: [id], onDelete: Cascade)
}

model vc_associates {
  id            String      @id
  uid           String
  associate_uid String
  createdAt     DateTime    @default(now())
  updatedAt     DateTime    @default(now())
  vc_partners   vc_partners @relation(fields: [associate_uid], references: [id], onDelete: Cascade)
  vc_firms      vc_firms    @relation(fields: [uid], references: [id], onDelete: Cascade)
}

model vc_documents {
  id          String   @id
  firm_uid    String
  name        String
  s3_key      String
  url         String
  type        String
  size        Int
  uploaded_by String
  createdAt   DateTime @default(now())
  updatedAt   DateTime
  vc_firms    vc_firms @relation(fields: [firm_uid], references: [id], onDelete: Cascade)
}

model vc_firms {
  id                String              @id 
  uid               String?              @unique @default(uuid()) @db.Uuid
  email             String?             @unique
  password          String?
  firm_number       String?
  firm_name         String?
  firm_hq           String?
  found_year        Int?
  legal_structure   String?
  url               String? // website
  firm_admin        String? // full name
  admin_email       String?
  admin_phone       String?
  admin_title       String?
  firm_logo         String?
  is_verified       Boolean?            @default(false)
  created_at        DateTime? @default(now()) @db.Timestamptz(6)
  updated_at        DateTime? @default(now()) @db.Timestamptz(6)

  angel_users       angel_users[]
  vc_administrative vc_administrative[]
  vc_associates     vc_associates[]
  vc_documents      vc_documents[]
  vc_gp             vc_gp[]
  vc_onboard_1      vc_onboard_1?
  vc_onboard_2      vc_onboard_2?
  vc_partners       vc_partners[]
}

model vc_gp {
  id             String      @id
  uid            String
  gp_uid         String
  deal_id        String?
  deal_doc       String?
  deal_termsheet String?
  createdAt      DateTime    @default(now())
  updatedAt      DateTime    @default(now())
  vc_partners    vc_partners @relation(fields: [gp_uid], references: [id], onDelete: Cascade)
  vc_firms       vc_firms    @relation(fields: [uid], references: [id], onDelete: Cascade)
}

model vc_onboard_1 {
  id                String      @id
  firm_name         String
  entity_type       String
  incorporation_add String
  found_date        DateTime
  aum               String
  focus_stage       String
  thesis            String
  userId            String      @unique
  firmId            String      @unique
  createdAt         DateTime    @default(now())
  updatedAt         DateTime
  vc_firms          vc_firms    @relation(fields: [firmId], references: [id], onDelete: Cascade)
  angel_users       angel_users @relation(fields: [userId], references: [id], onDelete: Cascade)
}

model vc_onboard_2 {
  id          String      @id
  full_name   String
  dob         DateTime
  nationality String
  residence   String
  title       String
  linkedin    String
  email       String
  phone       String
  userId      String      @unique
  firmId      String      @unique
  createdAt   DateTime    @default(now())
  updatedAt   DateTime
  vc_firms    vc_firms    @relation(fields: [firmId], references: [id], onDelete: Cascade)
  angel_users angel_users @relation(fields: [userId], references: [id], onDelete: Cascade)
}

model vc_partners {
  id                                               String              @id
  firm_uid                                         String
  role                                             String
  partner_uid                                      String?
  email                                            String
  password                                         String?
  full_name                                        String
  phone                                            String
  status                                           String              @default("pending")
  angelUserId                                      String?
  createdAt                                        DateTime            @default(now())
  updatedAt                                        DateTime
  vc_administrative                                vc_administrative[]
  vc_associates                                    vc_associates[]
  vc_gp                                            vc_gp[]
  angel_users_vc_partners_angelUserIdToangel_users angel_users?        @relation("vc_partners_angelUserIdToangel_users", fields: [angelUserId], references: [id])
  vc_firms                                         vc_firms            @relation(fields: [firm_uid], references: [id], onDelete: Cascade)
  angel_users_vc_partners_partner_uidToangel_users angel_users?        @relation("vc_partners_partner_uidToangel_users", fields: [partner_uid], references: [id], onDelete: Cascade)
}

